{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./WebMol/webmol/", "./WebMol/README.md"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "destination": "./WebMol/doc/",
        "recurse": true
    }
}